<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 <ThingDef ParentName="PlantBase">
     <thingClass>Ruyu.CyberFlora.CyberPlant</thingClass>
     <defName>Cyber_Rose</defName>
     <label>cyber rose</label>
	  <description>A beautiful cyber rose, the rose has been a symbol of love since Earth's ancient era, enchanted by a beautiful red glow. Cyberplants have a very long lifespan and can grow in any temperature.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
	  <Flammability>0</Flammability>
      <Beauty>35</Beauty>
      <BeautyOutdoors>35</BeautyOutdoors>
      <Nutrition>0.05</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/CyberRose</texPath>
      <graphicClass>Graphic_Random</graphicClass>
	  <drawSize>0.8</drawSize>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
    </ingestible>
    <plant>
      <dieIfLeafless>false</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <sowWork>2000</sowWork> <!-- 100 ticks per live day -->
      <sowTags>
        <li>CyberDecorative</li>
      </sowTags>
	  <sowResearchPrerequisites Inherit="False">
        <li>Research_CyberFlora</li>
      </sowResearchPrerequisites>
      <growDays>3</growDays>
      <lifespanDaysPerGrowDays>10000</lifespanDaysPerGrowDays>
      <visualSizeRange>0.3~1.05</visualSizeRange>
      <wildOrder>1</wildOrder>
      <purpose>Beauty</purpose>
	  <neverBlightable>true</neverBlightable>
      <growMinGlow>1</growMinGlow>
      <growOptimalGlow>3</growOptimalGlow>
    </plant>
    <fertility>1</fertility>
     <comps>
        <li Class="CompProperties_Glower">
         <glowRadius>5.9</glowRadius>
         <glowColor>(220,20,60,0)</glowColor>
        </li>
     </comps>
 </ThingDef>
 
